{
  "name": "node_app_boilerplate",
  "version": "1.0.0",
  "description": "A express and typescript project starter",
  "scripts": {
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/bin/WWW.ts",
    "prod": "tsc && node ./build/bin/WWW.js",
    "start": "set NODE_ENV=production && npm run prod",
    "debug": "ts-node-dev --debug --inspect-brk=9229 --respawn --transpileOnly s./src/bin/WWW.ts"
  },
  "keywords": [
    "express",
    "typescript",
    "node"
  ],
  "author": "AbiralStha",
  "license": "ISC",
  "dependencies": {
    "async": "^2.6.3",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "gravatar": "^1.8.0",
    "jade": "^1.11.0",
    "jsonwebtoken": "^8.5.0",
    "mongoose": "^5.4.15",
    "morgan": "^1.9.1",
    "nodemailer": "^6.3.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pusher": "^2.2.1",
    "serve-favicon": "^2.5.0",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.3.3333",
    "validator": "^10.11.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/async": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.1",
